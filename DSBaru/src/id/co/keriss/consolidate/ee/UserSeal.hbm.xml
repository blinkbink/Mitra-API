<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping 
  SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="id.co.keriss.consolidate.ee.UserSeal" table="user_seal">
   <id name="id" column="id" type="java.lang.Long">
     <generator class="sequence">
      <param name="sequence">user_seal_id_seq</param>
     </generator>
    </id>
    <many-to-one name="seal" class="id.co.keriss.consolidate.ee.Seal" not-null="false"></many-to-one>
	<many-to-one name="eeuser" class="org.jpos.ee.User"	not-null="false"></many-to-one>
    <property name="status" type="boolean"> 
    	<column name="status" default="true"></column>
    </property>
  </class>
</hibernate-mapping>