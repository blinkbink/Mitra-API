<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping 
  SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="id.co.keriss.consolidate.ee.Kuser" table="kuser">
   <id name="id" column="id" type="java.lang.Long">
     <generator class="sequence">
      <param name="sequence">kuser_id_seq</param>
     </generator>
    </id>
    <many-to-one name="eeuser"
			class="org.jpos.ee.User" not-null="false"></many-to-one>
    <property name="kuser" type="string"></property>
    
  </class>
</hibernate-mapping>