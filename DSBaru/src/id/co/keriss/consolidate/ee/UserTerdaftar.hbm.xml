<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping 
  SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="id.co.keriss.consolidate.ee.UserTerdaftar" table="user_terdaftar">
    <id name="id" column="id" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">user_terdaftar_id_seq</param>
			</generator>
		</id>
    <property name="nik" type="string" />
    <property name="create_date" type="java.util.Date" />
	<many-to-one name="mitra" class="id.co.keriss.consolidate.ee.Mitra"
			not-null="false"></many-to-one>
  </class>
</hibernate-mapping>