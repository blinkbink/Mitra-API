<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping 
  SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping>
	<class name="id.co.keriss.consolidate.ee.Activation"
		table="activation_session">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">act_session_seq</param>
			</generator>
		</id>
		<property name="expire_time" type="java.util.Date"></property>
		<property name="create_time" type="java.util.Date"></property>
		<many-to-one name="mitra"
			class="id.co.keriss.consolidate.ee.Mitra" not-null="false"></many-to-one>
			
		<many-to-one name="preregistration"
			class="id.co.keriss.consolidate.ee.PreRegistration" not-null="false"></many-to-one>
		<property name="session_key" type="string"></property>	
		<property name="used" type="boolean"></property>

	</class>
</hibernate-mapping>